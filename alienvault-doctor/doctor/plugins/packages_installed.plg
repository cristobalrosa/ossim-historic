[properties]
name=Packages installed
description=Check if the packages are properly installed
category=system
type=command
command=dpkg -l
raw_limit=100

[Package status]
pattern=^(?!ii|hi)\s+(\S+)
conditions=@string@:=="";@info@
fail_if_empty=False
warning=Some packages are not properly installed
advice=Half installed or configured packages could be a sympton of an update that went wrong. Please check your apt logs
