[properties]
name=Agent Cache Files
description=Check the integrity of the agent cache
category=alienvault,platform
profiles=Sensor:>4.0
type=command
command=ls /var/ossim/agent_events | perl -pe "s/\.\d+\.\d+\.db//" | uniq -c

# Check the number of the agent cache files for the same host
[Agent cache files]
fail_if_empty=False
pattern=(\d+)\s+\d+\.\d+\.\d+\.\d+
conditions=@int@:<6
warning=Too many agent cache files for at least one server
advice=Some cache files must be removed to free disk space