[properties]
name=Reachable systems
description=Check for reachable systems using the API
category=alienvault,system
type=command
command=alienvault-api systems

[Unreachable systems]
pattern=^\S+\s-\s\S+\s-\s(\S+)\s-\s(\S+)
conditions=@info@;@string@:=="Reachable"
fail_if_empty=True
warning=There are one or more systems which are unreachable
advice=Ensure that the systems are up and running, and their network configuration is successful
