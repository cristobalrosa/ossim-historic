[properties]
name=Dummy packages
description=Check the dummy packages
category=alienvault,system
type=command
command=dpkg -l | egrep '^ii\s+alienvault\-(vmware|ami|hw)' | awk '{ print $2 }'
requires=@dpkg:alienvault-professional

[Dummy packages]
pattern=^(\S*)\s+$
conditions=@set@:@issubsetof@/etc/ossim/doctor/plugins/alienvault_dummies.list
warning=There isn't a dummy package hw/vmware/ami installed
advice=A dummy package is required
