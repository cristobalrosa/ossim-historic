[properties]
name=IO speed
description=Tries to detect low IO speed.
category=system
type=command
requires=@files:/sbin/hdparm
command=hdparm -t /dev/sda
raw_limit=100

[IO test]
pattern=.*=\s(\S+)\s\S+
conditions=@float@:>90.0
warning=The IO speed is too low
advice=Low IO speed could lead to perfomance issues

