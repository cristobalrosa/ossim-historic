<?xml version="1.0" encoding='UTF-8' ?>

<!--
    Nagios Daemon
    Host and Service alert
    source = syslog | nagios
-->
<plugin id="1525" process="nagios" type="detector" start="no" enable="yes">

    <!-- debian users should use sysklogd instead of syslog -->
    <startup>/etc/init.d/nagios start</startup>
    <shutdown>/etc/init.d/nagios stop</shutdown>

    <source>nagios</source>
    <interface>&interface;</interface>
    <sensor>&sensor;</sensor>
    <location>/var/log/nagios/nagios.log</location>
</plugin>

