<!ELEMENT directives (directive)>

<!ELEMENT directive (rule, actions?)>
<!ATTLIST directive
	id		ID		#IMPLIED
	name		NMTOKEN		#REQUIRED
>

<!ELEMENT rule (rules, actions?)>
<!ATTLIST rule
	name		ID		#IMPLIED
        type            NMTOKEN         #REQUIRED
	priority	NMTOKEN		#REQUIRED
	reliability	NMTOKEN		#REQUIRED
        time_out        NMTOKEN         #REQUIRED
        occurrence      NMTOKEN         #REQUIRED
 	from		NMTOKEN		#REQUIRED
	to		NMTOKEN		#REQUIRED
	port_from	NMTOKEN		#REQUIRED
	port_to		NMTOKEN		#REQUIRED
	plugin		NMTOKEN		#REQUIRED
	tplugin		NMTOKEN		#REQUIRED
>

<!ELEMENT rules (rule+)>

<!ELEMENT actions (action+)>

<!ELEMENT action>
