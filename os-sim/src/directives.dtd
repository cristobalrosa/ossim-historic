<?xml version='1.0' encoding='UTF-8' ?>
 
<!--
  <!DOCTYPE jbosscmp-jdbc PUBLIC
      "-//Ossim//DTD Ossim Directives 1.0//EN"
      "http://www.ossim.net/dtd/directives.dtd">
